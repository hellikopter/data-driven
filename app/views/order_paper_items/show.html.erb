<h1>Order Paper Item Detail</h1>
<h3><%= link_to "Edit", order_paper_item_edit_path(@order_paper_item[:id]) %></h3>

<%= render "order_paper_item_details" %>

<% unless @order_paper_item[:concepts].empty? %>
	<p>Concepts linked to this order paper item</p>
	<ul>
		<% @order_paper_item[:concepts].each do |concept| %>
			<li>
				<%= link_to "#{concept[:label]}", concept_path(concept[:id]) %>
			</li>
		<% end %>
	</ul>
<% end %>

<% unless @order_paper_item[:previousItemId].nil? %>
	<%= link_to "Previous Item", order_paper_item_path(@order_paper_item[:previousItemId]) %>
<% end %>

