<h1><%= @division[:title] %></h1>

<ul>
	<% @division[:members].each do |member|%>
		<li>
			<dl>
				<dt>
					<%= link_to person_path(member[:id]) do %>
						<%= member[:name] %>
					<% end %>
				</dt>
				<dd><%= member[:vote_value] == 'true' ? 'Content' : 'Not content' %></dd>
			</dl>
		</li>
	<% end %>
</ul>


